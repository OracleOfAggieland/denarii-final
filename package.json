    "next": "^14.2.30",
    "openai": "^5.10.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.4",
